@model PA_BlogProject_2021.Models.ViewModel.IndexViewModel
<section class="slider">
    <div class="container">
        <div class="row">
            <div class="col-lg-9 col-md-10">
            @foreach (var item in Model.Sliders)
            {

                <div class="block">
                    <span class="d-block mb-3 text-white text-capitalize">@item.Title</span>
                    <h1 class="animated fadeInUp mb-5">@item.SubTitle</h1>
                    <a href="#" target="_blank" class="btn btn-main animated fadeInUp btn-round-full">Get started<i class="btn-icon fa fa-angle-right ml-2"></i></a>
                </div>
            }
            </div>
        </div>
    </div>
</section>